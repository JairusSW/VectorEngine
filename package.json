{
  "name": "vectorengine",
  "version": "1.0.33",
  "description": "This is a minimal WebGL vector rendering engine written for AssemblyScript.  ",
  "main": "index.ts",
  "ascMain": "index.ts",
  "scripts": {
    "test": "dev",
    "build": "rollup -c",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battlelinegames/VectorEngine.git"
  },
  "keywords": [
    "AssemblyScript",
    "WebAssembly",
    "Wasm",
    "WebGL",
    "Vector",
    "Rendering"
  ],
  "author": "Rick Battagline <rick@battagline.com> (https://wasmbook.com)",
  "license": "MIT",
  "bin": "vecinit",
  "bugs": {
    "url": "https://github.com/battlelinegames/VectorEngine/issues"
  },
  "homepage": "https://wasmbook.com",
  "devDependencies": {
    "assemblyscript": "^0.18.28",
    "connect": "^3.7.0",
    "express": "^4.17.1",
    "node-minify": "^2.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "serve-static": "^1.14.1"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.18.30",
    "avro-js": "^1.10.2",
    "bufferutil": "^4.0.3",
    "localtunnel": "^2.0.1",
    "utf-8-validate": "^5.0.4",
    "ws": "^7.4.5"
  }
}
